<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="emails">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="email" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="to" type="xs:string" minOccurs="1" />
                            <xs:element name="from" type="xs:string" minOccurs="1" maxOccurs="1" />
                            <xs:element name="heading" type="xs:string" minOccurs="1" maxOccurs="1" />
                            <xs:element name="body" type="xs:string" minOccurs="1" maxOccurs="1" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>